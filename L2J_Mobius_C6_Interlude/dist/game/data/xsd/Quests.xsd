<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  
  <xs:element name="list">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="quest" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="lvlMin">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:integer" use="required"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="lvlMax">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:integer" use="required"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="questType">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:integer" use="required"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="entityName">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:string" use="required"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="contactNpc">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:integer" use="required"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="restrictions">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:string" use="required"/>
                </xs:complexType>
              </xs:element>
              <xs:element name="description">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:string" use="required"/>
                </xs:complexType>
              </xs:element>
			  <xs:element name="class">
                <xs:complexType>
                  <xs:attribute name="val" type="xs:string" use="required"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="id" type="xs:integer" use="required"/>
            <xs:attribute name="name" type="xs:string" use="required"/>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>
